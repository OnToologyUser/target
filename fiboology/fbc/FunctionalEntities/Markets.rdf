		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/AgentsAndPeople/Agents/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/Arrangements/IdentifiersAndIndices/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/Places/Facilities/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:versionIRI
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/20150501/FunctionalEntities/Markets/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fbc-fct-mkt;Exchange">
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-fct-mkt;Market"/>
		<rdfs:subClassOf
			rdf:nodeID="blank2"/>
		<rdfs:label
			rdf:datatype="&xsd;string">exchange</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;string">Securities Exchange Act of 1934, as amended 12 August 2012</fibo-fnd-utl-av:definitionOrigin>
		<skos:definition
			rdf:datatype="&xsd;string">any organization, association, or group of persons, whether incorporated or unincorporated, which constitutes, maintains, or provides a market place or facilities for bringing together purchasers and sellers of securities or for otherwise performing with respect to securities the functions commonly performed by a stock exchange as that term is generally understood, and includes the market place and the market facilities maintained by such exchange</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fbc-fct-mkt;Market">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-plc-fac;Venue"/>
		<rdfs:label
			rdf:datatype="&xsd;string">market</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a market is a venue that allows buyers and sellers of goods, commodities, services, etc. to conduct business, including markets where securities may be traded</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fbc-fct-mkt;MarketIdentifier">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-arr-id;Identifier"/>
		<rdfs:subClassOf
			rdf:nodeID="blank1"/>
		<rdfs:label
			rdf:datatype="&xsd;string">market identifier</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;string">ISO 10383, Securities and related financial instruments - Codes for exchanges and market identification (MIC), Third edition, 2012-10-01.</fibo-fnd-utl-av:definitionOrigin>
		<skos:altLabel
			rdf:datatype="&xsd;string">MIC</skos:altLabel>
		<skos:definition
			rdf:datatype="&xsd;string">identifier or code allocated to places of trade, i.e., stock exchanges, regulated markets, e.g., Electronic Trading Platforms (ECN), and unregulated markets, e.g., Automated Trading Systems (ATS)</skos:definition>
		<skos:prefLabel
			rdf:datatype="&xsd;string">market identifier</skos:prefLabel>
	</owl:Class>
	
	<owl:Restriction
		rdf:nodeID="blank1">
		<owl:minQualifiedCardinality
			rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
		<owl:onClass
			rdf:resource="&fibo-fbc-fct-mkt;Exchange"/>
		<owl:onProperty
			rdf:resource="&fibo-fnd-aap-agt;identifies"/>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank2">
		<owl:minQualifiedCardinality
			rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
		<owl:onClass
			rdf:resource="&fibo-fbc-fct-mkt;MarketIdentifier"/>
		<owl:onProperty
			rdf:resource="&fibo-fnd-aap-agt;isIdentifiedBy"/>
	</owl:Restriction>

