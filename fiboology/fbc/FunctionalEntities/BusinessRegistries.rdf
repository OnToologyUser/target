		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/Relations/Relations/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/Utilities/BusinessFacingTypes/"/>
		<owl:versionIRI
			rdf:resource="http://spec.edmcouncil.org/fibo/FBC/20150501/FunctionalEntities/BusinessRegistries/"/>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-fbc-fct-breg;BusinessRegistrationAuthority">
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-fct-ra;RegistrationAuthority"/>
		<rdfs:subClassOf
			rdf:nodeID="blank2"/>
		<rdfs:subClassOf
			rdf:nodeID="blank3"/>
		<rdfs:label
			rdf:datatype="&xsd;string">business registration authority</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">a governmental registration authority that is responsible for maintaining a registry of business entities for a particular jurisdiction</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fbc-fct-breg;BusinessRegistry">
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-fct-ra;Registry"/>
		<rdfs:subClassOf
			rdf:nodeID="blank1"/>
		<rdfs:subClassOf
			rdf:nodeID="blank4"/>
		<rdfs:label
			rdf:datatype="&xsd;string">business registry</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">a registry for registering and maintaining information about business entities for a particular jurisdiction</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fbc-fct-breg;LegalRegistrationAddress">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-le-fbo;RegisteredAddress"/>
		<rdfs:label
			rdf:datatype="&xsd;string">legal registration address</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">an address for a business, such as its legal address, headquarters address, address registered for obtaining a license, and so forth</skos:definition>
	</owl:Class>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-fbc-fct-breg;hasAddressLine1">
		<rdfs:label
			rdf:datatype="&xsd;string">has address line 1</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:definitionOrigin>
		<rdfs:domain
			rdf:resource="&fibo-fbc-fct-breg;LegalRegistrationAddress"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-utl-bt;text"/>
		<skos:definition
			rdf:datatype="&xsd;string">the first line of the street address</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-fbc-fct-breg;hasAddressLine2">
		<rdfs:label
			rdf:datatype="&xsd;string">has address line 2</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:definitionOrigin>
		<rdfs:domain
			rdf:resource="&fibo-fbc-fct-breg;LegalRegistrationAddress"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-utl-bt;text"/>
		<skos:definition
			rdf:datatype="&xsd;string">the second line of the street address</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-fbc-fct-breg;hasAddressLine3">
		<rdfs:label
			rdf:datatype="&xsd;string">has address line 3</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:definitionOrigin>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">This element SHALL be omitted if
address line 2 is omitted.</fibo-fnd-utl-av:explanatoryNote>
		<rdfs:domain
			rdf:resource="&fibo-fbc-fct-breg;LegalRegistrationAddress"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-utl-bt;text"/>
		<skos:definition
			rdf:datatype="&xsd;string">the third line of the street address</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-fbc-fct-breg;hasAddressLine4">
		<rdfs:label
			rdf:datatype="&xsd;string">has address line 4</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:definitionOrigin>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">This element SHALL be omitted if
address line 3 is omitted.</fibo-fnd-utl-av:explanatoryNote>
		<rdfs:domain
			rdf:resource="&fibo-fbc-fct-breg;LegalRegistrationAddress"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-utl-bt;text"/>
		<skos:definition
			rdf:datatype="&xsd;string">the fourth line of the street address</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-fbc-fct-breg;hasCity">
		<rdfs:label
			rdf:datatype="&xsd;string">has city</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:definitionOrigin>
		<rdfs:domain
			rdf:resource="&fibo-fbc-fct-breg;LegalRegistrationAddress"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-utl-bt;text"/>
		<skos:definition
			rdf:datatype="&xsd;string">the name of the city</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fbc-fct-breg;hasCountryCode">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-rel-rel;has"/>
		<rdfs:label
			rdf:datatype="&xsd;string">has country code</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fbc-fct-breg;LegalRegistrationAddress"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-plc-cr;Alpha2Code"/>
		<skos:definition
			rdf:datatype="&xsd;string">identifies the ISO 3166-1 code for the country</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fbc-fct-breg;hasHeadquartersAddress">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-be-le-fbo;hasPrimaryAddress"/>
		<rdfs:label
			rdf:datatype="&xsd;string">has headquarters address</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fbc-fct-breg;LegalRegistrationAddress"/>
		<skos:definition
			rdf:datatype="&xsd;string">identifies the headquarters address for the registered entity</skos:definition>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-fbc-fct-breg;hasPostalCode">
		<rdfs:label
			rdf:datatype="&xsd;string">has postal code</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.leiroc.org/publications/gls/lou_20140620.pdf</fibo-fnd-utl-av:definitionOrigin>
		<rdfs:domain
			rdf:resource="&fibo-fbc-fct-breg;LegalRegistrationAddress"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-utl-bt;text"/>
		<skos:definition
			rdf:datatype="&xsd;string">the postal code of this address as specified by the local postal service</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-fbc-fct-breg;hasRegistrationStatus">
		<rdfs:label
			rdf:datatype="&xsd;string">has registration status</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fnd-utl-bt;text"/>
		<skos:definition
			rdf:datatype="&xsd;string">the status of a specific registration, such as a registered identifier or license, in the registry</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fbc-fct-breg;hasRegistrationStatusRevisionDate">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-dt-fd;hasDate"/>
		<rdfs:label
			rdf:datatype="&xsd;string">has registration status revision date</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fnd-dt-fd;Date"/>
		<skos:definition
			rdf:datatype="&xsd;string">the date that the status of a specific registration in the registry was revised</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fbc-fct-breg;hasSubdivisionCode">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-rel-rel;has"/>
		<rdfs:label
			rdf:datatype="&xsd;string">has subdivision code</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fbc-fct-breg;LegalRegistrationAddress"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-plc-cr;SubdivisionCode"/>
		<skos:definition
			rdf:datatype="&xsd;string">identifies the code for the country subdivision (state, province, region, etc.)</skos:definition>
	</owl:ObjectProperty>
	
	<owl:Restriction
		rdf:nodeID="blank1">
		<owl:onClass
			rdf:resource="&fibo-fbc-fct-breg;BusinessRegistrationAuthority"/>
		<owl:onProperty
			rdf:resource="&fibo-fnd-rel-rel;isManagedBy"/>
		<owl:qualifiedCardinality
			rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank2">
		<owl:onClass
			rdf:resource="&fibo-fbc-fct-breg;BusinessRegistry"/>
		<owl:onProperty
			rdf:resource="&fibo-fnd-rel-rel;manages"/>
		<owl:qualifiedCardinality
			rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank3">
		<owl:onClass
			rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
		<owl:onProperty
			rdf:resource="&fibo-fnd-rel-rel;isGovernedBy"/>
		<owl:qualifiedCardinality
			rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank4">
		<owl:onClass
			rdf:resource="&fibo-fnd-law-jur;Jurisdiction"/>
		<owl:onProperty
			rdf:resource="&fibo-fnd-rel-rel;isGovernedBy"/>
		<owl:qualifiedCardinality
			rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
	</owl:Restriction>

</rdf:RDF>