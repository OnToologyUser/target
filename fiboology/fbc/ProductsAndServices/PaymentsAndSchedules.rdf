	<owl:Class
		rdf:about="&fibo-fbc-pas-psch;Obligation">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-law-lcap;Duty"/>
		<rdfs:subClassOf
			rdf:nodeID="blank02"/>
		<rdfs:subClassOf
			rdf:nodeID="blank03"/>
		<rdfs:label
			rdf:datatype="&xsd;string">obligation</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">a legally enforceable duty to pay a sum of money, or agree to do something (or not to do something), according to the terms stated in a contract</skos:definition>
		<skos:example
			rdf:datatype="&xsd;string">the duty of a borrower to repay a loan, and the legal right of a lender to enforce payment</skos:example>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fbc-pas-psch;Payee">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
		<rdfs:subClassOf
			rdf:nodeID="blank11"/>
		<rdfs:label
			rdf:datatype="&xsd;string">payee</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">a party to whom a debt should be paid, or to whose order a bill of exchange, note, or check is made payable</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fbc-pas-psch;Payer">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
		<rdfs:subClassOf
			rdf:nodeID="blank01"/>
		<rdfs:label
			rdf:datatype="&xsd;string">payer</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">a party who pays a bill or fees</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fbc-pas-psch;Payment">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
		<rdfs:subClassOf
			rdf:nodeID="blank05"/>
		<rdfs:subClassOf
			rdf:nodeID="blank09"/>
		<rdfs:subClassOf
			rdf:nodeID="blank10"/>
		<rdfs:label
			rdf:datatype="&xsd;string">payment</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">delivery of money in fulfillment of an obligation, such as to satisfy a claim or debt</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fbc-pas-psch;PaymentEvent">
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-pas-pas;Transaction"/>
		<rdfs:subClassOf
			rdf:nodeID="blank06"/>
		<rdfs:subClassOf
			rdf:nodeID="blank07"/>
		<rdfs:label
			rdf:datatype="&xsd;string">payment event</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">an event that involves delivery of money in fulfillment of an obligation</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fbc-pas-psch;PaymentSchedule">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-dt-fd;Schedule"/>
		<rdfs:subClassOf
			rdf:nodeID="blank08"/>
		<rdfs:label
			rdf:datatype="&xsd;string">payment schedule</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">schedule for delivery of money in fulfillment of an obligation, such as a coupon payment schedule, loan payment schedule, interest payment schedule</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fbc-pas-psch;fulfillsObligation">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-rel-rel;refersTo"/>
		<rdfs:label
			rdf:datatype="&xsd;string">fulfills obligation</rdfs:label>
