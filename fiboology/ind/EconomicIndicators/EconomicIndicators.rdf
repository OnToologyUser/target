	<owl:Class
		rdf:about="&fibo-ind-ei-ei;ConsumerPriceIndex">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;EconomicIndicator"/>
		<rdfs:subClassOf
			rdf:nodeID="blank8"/>
		<rdfs:label
			rdf:datatype="&xsd;string">consumer price index</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://stats.bls.gov/cpi/cpifaq.htm#Question_1</fibo-fnd-utl-av:definitionOrigin>
		<owl:disjointWith
			rdf:resource="&fibo-ind-ei-ei;GrossDomesticProduct"/>
		<owl:disjointWith
			rdf:resource="&fibo-ind-ei-ei;UnemploymentRate"/>
		<skos:altLabel
			rdf:datatype="&xsd;string">CPI</skos:altLabel>
		<skos:definition
			rdf:datatype="&xsd;string">an economic indicator representing a measure of the average change over time in the prices paid by urban consumers for a market basket of consumer goods and services</skos:definition>
		<skos:prefLabel
			rdf:datatype="&xsd;string">consumer price index</skos:prefLabel>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;EconomicActivity">
		<rdfs:subClassOf
			rdf:nodeID="blank5"/>
		<rdfs:label
			rdf:datatype="&xsd;string">economic activity</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an aspect of the behavior of some economic actor in an economy, related to the production, distribution, and consumption of goods and services</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;EconomicIndicator">
		<rdfs:subClassOf
			rdf:resource="&fibo-be-fct-pub;Publication"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-utl-alx;StatisticalMeasure"/>
		<rdfs:subClassOf
			rdf:nodeID="blank6"/>
		<rdfs:subClassOf
			rdf:nodeID="blank7"/>
		<rdfs:label
			rdf:datatype="&xsd;string">economic indicator</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a statistical measure of an economic activity, used for analysis of economic performance and predictions of future performance</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;Economy">
		<rdfs:subClassOf
			rdf:nodeID="blank4"/>
		<rdfs:label
			rdf:datatype="&xsd;string">economy</rdfs:label>
		<skos:altLabel
			rdf:datatype="&xsd;string">economic system</skos:altLabel>
		<skos:definition
			rdf:datatype="&xsd;string">An economy consists of the production, distribution or trade, and consumption of limited goods and services by different economic actors in a given geographically distinct web of relationships.</skos:definition>
		<skos:prefLabel
			rdf:datatype="&xsd;string">economy</skos:prefLabel>
		<skos:scopeNote
			rdf:datatype="&xsd;string">This may be a national economy (relating to a country); a regional economy, or the economy of a state or some other intra-national entity. It need not always be geographical in the strictest sense, for example Second Life has an economy. For the purposes of this model, it refers only to economies involving economic exchange i.e. not the broader sense used by evolutionary biologists etc.</skos:scopeNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;GrossDomesticProduct">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;EconomicIndicator"/>
		<rdfs:subClassOf
			rdf:nodeID="blank1"/>
		<rdfs:subClassOf
			rdf:nodeID="blank2"/>
		<rdfs:label
			rdf:datatype="&xsd;string">gross domestic product</rdfs:label>
		<fibo-fnd-utl-av:definitionOrigin
			rdf:datatype="&xsd;anyURI">http://www.treasury.gov/initiatives/ofr/about/Documents/AR2013_Back_Matter_Glossary+Bib_Refs+Endnotes.pdf</fibo-fnd-utl-av:definitionOrigin>
		<owl:disjointWith
			rdf:resource="&fibo-ind-ei-ei;UnemploymentRate"/>
		<skos:altLabel
			rdf:datatype="&xsd;string">GDP</skos:altLabel>
		<skos:definition
			rdf:datatype="&xsd;string">an economic indicator representing the broadest measure of aggregate economic activity, measuring the total value of all final goods and services produced within a country&apos;s borders during a specific period</skos:definition>
		<skos:prefLabel
			rdf:datatype="&xsd;string">gross domestic product</skos:prefLabel>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;InflationRate">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-utl-bt;Percentage"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;EconomicIndicator"/>
		<rdfs:subClassOf
			rdf:nodeID="blank3"/>
		<rdfs:label
			rdf:datatype="&xsd;string">inflation rate</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an economic indicator representing a percent change in consumer prices for a specified, typically a one-year, period, for a given country</skos:definition>
		<skos:scopeNote
			rdf:datatype="&xsd;string">Always either includes or excludes: Energy prices; Food prices. ALL inflation rates cite whether or not they exclude energy and food prices. If nothing stated it is assumed they include them.</skos:scopeNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;NumericUnemploymentRate">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;UnemploymentRate"/>
		<rdfs:subClassOf
			rdf:nodeID="blank9"/>
		<rdfs:label
			rdf:datatype="&xsd;string">numeric unemployment rate</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an economic indicator representing the number of unemployed in the labor force of a given economy for some specified period</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;PercentageUnemploymentRate">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-utl-bt;Percentage"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;UnemploymentRate"/>
		<rdfs:label
			rdf:datatype="&xsd;string">percentage unemployment rate</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an economic indicator representing the ratio of unemployment to the labor force of a given economy for some specified period expressed in percentage terms</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-ei-ei;UnemploymentRate">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ei-ei;EconomicIndicator"/>
		<rdfs:label
			rdf:datatype="&xsd;string">unemployment rate</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">http://www.bls.gov/cps/faq.htm#Ques3</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">Persons are classified as unemployed if they do not have a job, have actively looked for work in the prior 4 weeks, and are currently available for work. Workers expecting to be recalled from layoff are counted as unemployed, whether or not they have engaged in a specific jobseeking activity. In all other cases, the individual must have been engaged in at least one active job search activity in the 4 weeks preceding the interview and be available for work (except for temporary illness).</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">an economic indicator representing the level of unemployment to the labor force of a given economy for some specified period</skos:definition>
	</owl:Class>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-ind-ei-ei;excludesEnergyAndfood">
		<rdfs:label
			rdf:datatype="&xsd;string">excludes energy and food</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-ind-ei-ei;ConsumerPriceIndex"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
		<skos:definition
			rdf:datatype="&xsd;string">a predicate indicating whether the index excludes energy and food prices</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-ind-ei-ei;isEconomyOf">
		<rdfs:label
			rdf:datatype="&xsd;string">is economy of</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-ind-ei-ei;Economy"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-plc-cty;GeopoliticalEntity"/>
		<skos:definition
			rdf:datatype="&xsd;string">a predicate relating an economy to the geopolitical entity it refers to</skos:definition>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-ind-ei-ei;isSeasonallyAdjusted">
		<rdfs:label
			rdf:datatype="&xsd;string">is seasonally adjusted</rdfs:label>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">Example explanation from the US Bureau of Labor and Statistics: Because price data are used for different purposes by different groups, the Bureau of Labor Statistics publishes seasonally adjusted as well as unadjusted changes each month. ...  Seasonal factors used in computing the seasonally adjusted indexes are derived by the X-13ARIMA-SEATS Seasonal Adjustment Method.  Seasonally adjusted indexes and seasonal factors are computed annually.  Each year, the last five years of seasonally adjusted data are revised.</fibo-fnd-utl-av:explanatoryNote>
		<rdfs:domain
			rdf:resource="&fibo-ind-ei-ei;EconomicIndicator"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-utl-bt;yesOrNo"/>
		<skos:definition
			rdf:datatype="&xsd;string">a predicate indicating whether some published formal method is applied that compensates for seasonal variations in the index value</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:Restriction
		rdf:nodeID="blank1">
		<owl:allValuesFrom
			rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
		<owl:onProperty
			rdf:resource="&fibo-fnd-acc-cur;hasNotionalAmount"/>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank2">
		<owl:onClass
			rdf:resource="&fibo-fnd-plc-cty;Country"/>
		<owl:onProperty
			rdf:resource="&fibo-fnd-rel-rel;appliesTo"/>
		<owl:qualifiedCardinality
			rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank3">
		<owl:onClass
			rdf:resource="&fibo-fnd-plc-cty;Country"/>
		<owl:onProperty
			rdf:resource="&fibo-fnd-rel-rel;appliesTo"/>
		<owl:qualifiedCardinality
			rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank4">
		<owl:onProperty
			rdf:resource="&fibo-fnd-rel-rel;appliesTo"/>
		<owl:someValuesFrom
			rdf:resource="&fibo-fnd-plc-loc;Location"/>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank5">
		<owl:onProperty
			rdf:resource="&fibo-fnd-rel-rel;appliesTo"/>
		<owl:someValuesFrom
			rdf:resource="&fibo-ind-ei-ei;Economy"/>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank6">
		<owl:onProperty
			rdf:resource="&fibo-fnd-rel-rel;appliesTo"/>
		<owl:someValuesFrom
			rdf:resource="&fibo-ind-ei-ei;Economy"/>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank7">
		<owl:onProperty
			rdf:resource="&fibo-fnd-utl-alx;isMeasureOf"/>
		<owl:someValuesFrom
			rdf:resource="&fibo-ind-ei-ei;EconomicActivity"/>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank8">
		<owl:onProperty
			rdf:resource="&fibo-fnd-utl-bt;hasNumericValue"/>
		<owl:someValuesFrom
			rdf:resource="&fibo-fnd-utl-bt;number"/>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank9">
		<owl:onProperty
			rdf:resource="&fibo-fnd-utl-bt;hasNumericValue"/>
		<owl:someValuesFrom
			rdf:resource="&fibo-fnd-utl-bt;number"/>
	</owl:Restriction>

</rdf:RDF>