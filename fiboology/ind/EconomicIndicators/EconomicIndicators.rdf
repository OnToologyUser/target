	xmlns:dct="http://purl.org/dc/terms/"
	xmlns:fibo-be-fct-pub="http://spec.edmcouncil.org/fibo/BE/FunctionalEntities/Publishers/"
	xmlns:fibo-fnd-acc-cur="http://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"
