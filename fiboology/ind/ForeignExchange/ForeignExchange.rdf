<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY dct "http://purl.org/dc/terms/">
	<!ENTITY fibo-fnd-acc-cur "http://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/">
	<!ENTITY fibo-fnd-dt-fd "http://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/">
	<!ENTITY fibo-fnd-rel-rel "http://spec.edmcouncil.org/fibo/FND/Relations/Relations/">
	<!ENTITY fibo-fnd-utl-alx "http://spec.edmcouncil.org/fibo/FND/Utilities/Analytics/">
	<!ENTITY fibo-fnd-utl-av "http://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/">
	<!ENTITY fibo-fnd-utl-bt "http://spec.edmcouncil.org/fibo/FND/Utilities/BusinessFacingTypes/">
	<!ENTITY fibo-ind-fx-fx "http://spec.edmcouncil.org/fibo/IND/ForeignExchange/ForeignExchange/">
	<!ENTITY fibo-ind-ind-ind "http://spec.edmcouncil.org/fibo/IND/Indicators/Indicators/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY sm "http://www.omg.org/techprocess/ab/SpecificationMetadata/">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF
	xmlns:dct="http://purl.org/dc/terms/"
	xmlns:fibo-fnd-acc-cur="http://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"
	xmlns:fibo-fnd-dt-fd="http://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"
	xmlns:fibo-fnd-rel-rel="http://spec.edmcouncil.org/fibo/FND/Relations/Relations/"
	xmlns:fibo-fnd-utl-alx="http://spec.edmcouncil.org/fibo/FND/Utilities/Analytics/"
	xmlns:fibo-fnd-utl-av="http://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"
	xmlns:fibo-fnd-utl-bt="http://spec.edmcouncil.org/fibo/FND/Utilities/BusinessFacingTypes/"
	xmlns:fibo-ind-fx-fx="http://spec.edmcouncil.org/fibo/IND/ForeignExchange/ForeignExchange/"
	xmlns:fibo-ind-ind-ind="http://spec.edmcouncil.org/fibo/IND/Indicators/Indicators/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
	xml:space="preserve">
	
	<owl:Ontology
		rdf:about="http://spec.edmcouncil.org/fibo/IND/ForeignExchange/ForeignExchange/">
		<rdfs:label
			rdf:datatype="&xsd;string">Foreign Exchange Ontology</rdfs:label>
		<dct:license
			rdf:datatype="&xsd;anyURI">http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense</dct:license>
		<sm:contentLanguage
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/ODM/</sm:contentLanguage>
		<sm:contentLanguage
			rdf:datatype="&xsd;anyURI">http://www.w3.org/standards/techs/owl#w3c_all</sm:contentLanguage>
		<sm:copyright
			rdf:datatype="&xsd;string">Copyright (c) 2014-2015 EDM Council, Inc.
Copyright (c) 2014-2015 Object Management Group, Inc.</sm:copyright>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/fibo/FND/</sm:dependsOn>
		<sm:dependsOn
			rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/fibo/IND/Indicators/Indicators/</sm:dependsOn>
		<sm:fileAbbreviation
			rdf:datatype="&xsd;string">fibo-ind-fx-fx</sm:fileAbbreviation>
		<sm:fileAbstract
			rdf:datatype="&xsd;string">This ontology provides the parameters for foreign exchange rates, covering spot and forward rates, as well as Fx spot rate volatilities.</sm:fileAbstract>
		<sm:filename
			rdf:datatype="&xsd;string">ForeignExchange.rdf</sm:filename>
		<rdfs:seeAlso
			rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/fibo/AboutTheEDMC-FIBOFamily/</rdfs:seeAlso>
		<rdfs:seeAlso
			rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/fibo/IND/AboutIND/</rdfs:seeAlso>
		<rdfs:seeAlso
			rdf:datatype="&xsd;anyURI">http://spec.edmcouncil.org/fibo/IND/ForeignExchange/AboutForeignExchange/</rdfs:seeAlso>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/Accounting/CurrencyAmount/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/DatesAndTimes/FinancialDates/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/Relations/Relations/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/Utilities/Analytics/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/FND/Utilities/BusinessFacingTypes/"/>
		<owl:imports
			rdf:resource="http://spec.edmcouncil.org/fibo/IND/Indicators/Indicators/"/>
		<owl:versionIRI
			rdf:resource="http://spec.edmcouncil.org/fibo/IND/20150501/ForeignExchange/ForeignExchange/"/>
		<skos:changeNote
			rdf:datatype="&xsd;string">The http://spec.edmcouncil.org/fibo/IND/20140601/ForeignExchange/ForeignExchange.rdf version of this ontology was modified per the issue resolutions identified in the FIBO IND 1.0 FTF report.</skos:changeNote>
	</owl:Ontology>
	
	<owl:Class
		rdf:about="&fibo-ind-fx-fx;CurrencyForwardRate">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-utl-bt;Percentage"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-fx-fx;ExchangeRate"/>
		<rdfs:subClassOf
			rdf:nodeID="blank1"/>
		<rdfs:label
			rdf:datatype="&xsd;string">currency forward rate</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a rate of exchange between two currencies for settlement at some future point in time, expressed as a premium on the spot rate</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-fx-fx;CurrencySpotBuyRate">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-fx-fx;ExchangeRate"/>
		<rdfs:label
			rdf:datatype="&xsd;string">currency spot buy rate</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an indicative spot buying market rate as observed by the reporting source</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-fx-fx;CurrencySpotMidRate">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-fx-fx;ExchangeRate"/>
		<rdfs:label
			rdf:datatype="&xsd;string">currency spot mid rate</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an indicative middle market (mean of spot buying and selling) rate as observed by the reporting source</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-fx-fx;CurrencySpotSellRate">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-fx-fx;ExchangeRate"/>
		<rdfs:label
			rdf:datatype="&xsd;string">currency spot sell rate</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an indicative spot selling market rate as observed by the reporting source</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-fx-fx;ExchangeRate">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ind-ind;MarketRate"/>
		<rdfs:subClassOf
			rdf:nodeID="blank3"/>
		<rdfs:subClassOf
			rdf:nodeID="blank4"/>
		<rdfs:subClassOf
			rdf:nodeID="blank5"/>
		<rdfs:label
			rdf:datatype="&xsd;string">exchange rate</rdfs:label>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">For example an exchange rate of R represents a rate of R units of the dealt currency to 1 unit of the base currency.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">An indicative, quoted exchange rate at a point in time, for a given block amount of currency as quoted against another (base) currency, for settlement at a defined point in the immediate future.</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-ind-fx-fx;FxSpotVolatility">
		<rdfs:subClassOf
			rdf:resource="&fibo-ind-ind-ind;Volatility"/>
		<rdfs:subClassOf
			rdf:nodeID="blank2"/>
		<rdfs:label
			rdf:datatype="&xsd;string">FX spot volatility</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;anyURI">OptionsEducation.org</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">A measure of exchange rate fluctuation. Mathematically the volatility is the annualized standard deviation of the daily changes in the exchange rate.</skos:definition>
	</owl:Class>
	
	<rdf:Description
		rdf:about="&fibo-ind-fx-fx;hasBaseCurrency">
		<rdf:type
			rdf:resource="&owl;InverseFunctionalProperty"/>
		<rdf:type
			rdf:resource="&owl;ObjectProperty"/>
		<rdfs:label
			rdf:datatype="&xsd;string">has base currency</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-cur;Currency"/>
		<skos:definition
			rdf:datatype="&xsd;string">a predicate indicating the base currency in an exchange rate; one unit of this currency represents R units of the dealt currency, where R is the exchange rate value</skos:definition>
	</rdf:Description>
	
	<owl:ObjectProperty
		rdf:about="&fibo-ind-fx-fx;hasQuotationBlockAmountBasis">
		<rdfs:label
			rdf:datatype="&xsd;string">has quotation block amount basis</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-ind-fx-fx;ExchangeRate"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-cur;MonetaryAmount"/>
		<skos:definition
			rdf:datatype="&xsd;string">the amount of the dealt currency which would be exchanged in a trade for which the stated spot rate applies</skos:definition>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-ind-fx-fx;hasQuotationSettlementBasisInDays">
		<rdfs:label
			rdf:datatype="&xsd;string">has quotation settlement basis in days</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-ind-fx-fx;ExchangeRate"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-utl-bt;number"/>
		<skos:definition
			rdf:datatype="&xsd;string">the settlement period in days for a trade for which the stated spot rate applies</skos:definition>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-ind-fx-fx;hasQuoteCurrency">
		<rdfs:label
			rdf:datatype="&xsd;string">has quote currency</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-cur;Currency"/>
		<skos:definition
			rdf:datatype="&xsd;string">a predicate indicating the quote currency in an exchange rate; R units of this currency represent one unit of the base currency</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-ind-fx-fx;hasSettlementDate">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-dt-fd;hasDate"/>
		<rdfs:label
			rdf:datatype="&xsd;string">has settlement date</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fnd-dt-fd;Date"/>
		<skos:definition
			rdf:datatype="&xsd;string">a predicate indicating the settlement date of a given transaction</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-ind-fx-fx;isPremiumOn">
		<rdfs:label
			rdf:datatype="&xsd;string">is premium on</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-ind-fx-fx;CurrencyForwardRate"/>
		<rdfs:range
			rdf:resource="&fibo-ind-fx-fx;ExchangeRate"/>
		<skos:definition
			rdf:datatype="&xsd;string">an exchange rate expressed as a premium on the spot rate for the currency pair</skos:definition>
	</owl:ObjectProperty>
	
	<owl:Restriction
		rdf:nodeID="blank1">
		<owl:allValuesFrom
			rdf:resource="&fibo-fnd-dt-fd;Date"/>
		<owl:onProperty
			rdf:resource="&fibo-ind-fx-fx;hasSettlementDate"/>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank2">
		<owl:allValuesFrom
			rdf:resource="&fibo-ind-fx-fx;ExchangeRate"/>
		<owl:onProperty
			rdf:resource="&fibo-ind-ind-ind;isVolatilityOf"/>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank3">
		<owl:onProperty
			rdf:resource="&fibo-fnd-utl-bt;hasNumericValue"/>
		<owl:someValuesFrom
			rdf:resource="&fibo-fnd-utl-bt;number"/>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank4">
		<owl:onProperty
			rdf:resource="&fibo-ind-fx-fx;hasBaseCurrency"/>
		<owl:someValuesFrom
			rdf:resource="&fibo-fnd-acc-cur;Currency"/>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank5">
		<owl:onProperty
			rdf:resource="&fibo-ind-fx-fx;hasQuoteCurrency"/>
		<owl:someValuesFrom
			rdf:resource="&fibo-fnd-acc-cur;Currency"/>
	</owl:Restriction>

</rdf:RDF>