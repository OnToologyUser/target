	<owl:Ontology
		rdf:about="http://spec.edmcouncil.org/fibo/BE/OwnershipAndControl/CorporateControl/">
		<rdfs:label
			rdf:datatype="&xsd;string">Corporate Control Ontology</rdfs:label>
		<dct:license
			rdf:datatype="&xsd;anyURI">http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense</dct:license>
		<sm:contentLanguage
			rdf:datatype="&xsd;anyURI">http://www.omg.org/spec/ODM/</sm:contentLanguage>
