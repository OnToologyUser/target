	<owl:Class
		rdf:about="&fibo-be-sps-sps;SoleProprietor">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-oac-own;Owner"/>
		<rdfs:subClassOf
			rdf:nodeID="blank3"/>
		<rdfs:subClassOf
			rdf:nodeID="blank4"/>
		<rdfs:label
			rdf:datatype="&xsd;string">sole proprietor</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">A sole proprietor has unlimited liability with respect to any business debts.</fibo-fnd-utl-av:explanatoryNote>
		<fibo-fnd-utl-av:synonym
			rdf:datatype="&xsd;string">sole owner</fibo-fnd-utl-av:synonym>
		<skos:definition
			rdf:datatype="&xsd;string">a party that owns a business, has the rights to all profits from that business and is considered a single entity (unincorporated) together with that business for tax and liability purposes</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-be-sps-sps;SoleProprietorship">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-org-fm;FormalOrganization"/>
		<rdfs:subClassOf
			rdf:nodeID="blank2"/>
		<rdfs:label
			rdf:datatype="&xsd;string">sole proprietorship</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">an unincorporated business owned by a single person</skos:definition>
	</owl:Class>
	
	<owl:Restriction
		rdf:nodeID="blank1">
		<owl:minQualifiedCardinality
			rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
		<owl:onClass
