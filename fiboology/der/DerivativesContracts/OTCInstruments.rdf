	<owl:Class
		rdf:about="&fibo-der-drc-bsc;OTCTransactionParty">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agr-ctr;ContractParty"/>
		<rdfs:subClassOf
			rdf:nodeID="blank3"/>
		<rdfs:subClassOf
			rdf:nodeID="blank6"/>
		<rdfs:label
			rdf:datatype="&xsd;string">OTC transaction party</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a party to an over-the-counter agreement</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-drc-otci;CashDeliveryCommitment">
		<rdfs:subClassOf
			rdf:resource="&fibo-der-drc-bsc;DeliveryCommitment"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-pas-psch;Obligation"/>
		<rdfs:subClassOf
			rdf:nodeID="blank1"/>
		<rdfs:label
			rdf:datatype="&xsd;string">cash delivery commitment</rdfs:label>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">This is the same date as a settlement date when the corresponding cash is made available in the other currency, in settlement for this amount. FpML exchangedCurrency referred to as a &quot;Currency Stream&quot;</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">commitment to deliver cash at the earliest available date as per settlement convention</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-drc-otci;DeliverCash">
		<rdfs:subClassOf
			rdf:resource="&fibo-der-drc-bsc;DeliveryTermsSet"/>
		<rdfs:label
			rdf:datatype="&xsd;string">deliver cash</rdfs:label>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-drc-otci;OTCContractSettlementTerms">
		<rdfs:subClassOf
			rdf:resource="&fibo-der-drc-bsc;SettlementTermsSet"/>
		<rdfs:label
			rdf:datatype="&xsd;string">OTC settlement terms</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">settlement terms specific to an over-the-counter agreement</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-drc-otci;OTCDerivativeContract">
		<rdfs:subClassOf
			rdf:resource="&fibo-der-drc-otci;OTCInstrument"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-fi-fi;DerivativeInstrument"/>
		<rdfs:label
			rdf:datatype="&xsd;string">OTC derivative contract</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an over-the-counter agreement that is also a derivative instrument</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xsd;string">Not all derivatives are sold over-the-counter, and not all OTC agreements are derivatives.</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-drc-otci;OTCInstrument">
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-fi-fi;FinancialInstrument"/>
		<rdfs:label
			rdf:datatype="&xsd;string">OTC instrument</rdfs:label>
		<fibo-fnd-utl-av:adaptedFrom
			rdf:datatype="&xsd;string">Barron&apos;s Dictionary of Finance and Investment Terms, Ninth Edition, 2014</fibo-fnd-utl-av:adaptedFrom>
		<skos:definition
			rdf:datatype="&xsd;string">an instrument that is not listed and traded on an organized exchange</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-drc-otci;OTCInstrumentConfirmation">
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-pas-pas;TransactionConfirmation"/>
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-agr-ctr;WrittenContract"/>
		<rdfs:subClassOf
			rdf:nodeID="blank2"/>
		<rdfs:subClassOf
			rdf:nodeID="blank5"/>
		<rdfs:label
			rdf:datatype="&xsd;string">OTC instrument confirmation</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">a formal confirmation document which constitutes a written contract between the parties to the transaction, setting out the terms and conditions of the transaction</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xsd;string">This effectively supersedes the (implied) contract that is brought into being by the transaction. It makes reference to the terms and conditions under which the transaction is deemed to have been conducted. The Confirmation references the Master Agreement under which the transaction has been conducted along with the ISDA standard terms referred to (if applicable), and adds terms and conditions specific to that one Transaction. This Confirmation generally supersedes all terms in the ISDA standard Definitions and the Master Agreement in the event of any inconsistency between the three.</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-der-drc-otci;OTCTransaction">
		<rdfs:subClassOf
			rdf:resource="&fibo-fbc-fi-fi;SecuritiesTransaction"/>
		<rdfs:subClassOf
			rdf:nodeID="blank7"/>
		<rdfs:label
			rdf:datatype="&xsd;string">OTC transaction</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">an over-the-counter transaction to exchange an instrument for some consideration</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xsd;string">Removed inheritance relationship with Transactions/TransactionsExtended/CoveredTransaction for the sake of expediency.</skos:editorialNote>
		<skos:editorialNote
			rdf:datatype="&xsd;string">This represents a covered transaction, that is, the OTC transaction is transacted under some previously agreed Master Agreement between the two parties.</skos:editorialNote>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-der-drc-otci;hasDeliveryAmount">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-rel-rel;has"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-acc-cur;MoneyAmount"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-der-drc-otci;hasDeliveryDate">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-dt-fd;hasDate"/>
		<rdfs:label
			rdf:datatype="&xsd;string">has delivery date</rdfs:label>
		<rdfs:range
			rdf:resource="&fibo-fnd-dt-fd;Date"/>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty
		rdf:about="&fibo-der-drc-otci;hasPaymentType">
		<rdfs:range
			rdf:resource="&xsd;string"/>
	</owl:DatatypeProperty>
	
	<owl:Restriction
		rdf:nodeID="blank1">
		<owl:allValuesFrom
			rdf:resource="&fibo-der-drc-otci;DeliverCash"/>
		<owl:onProperty
			rdf:resource="&fibo-fnd-rel-rel;refersTo"/>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank2">
		<owl:allValuesFrom
			rdf:resource="&fibo-der-drc-otci;OTCTransaction"/>
		<owl:onProperty
			rdf:resource="&fibo-fnd-rel-rel;governs"/>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank3">
		<owl:onClass
			rdf:resource="&fibo-be-le-lp;LegalPerson"/>
		<owl:onProperty
			rdf:resource="&fibo-fnd-rel-rel;hasIdentity"/>
		<owl:qualifiedCardinality
			rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank4">
		<owl:onClass
			rdf:resource="&fibo-der-drc-otci;OTCInstrument"/>
		<owl:onProperty
			rdf:resource="&fibo-fnd-pty-pty;isAPartyTo"/>
		<owl:qualifiedCardinality
			rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank5">
		<owl:onProperty
			rdf:resource="&fibo-fnd-agr-ctr;hasContractParty"/>
		<owl:someValuesFrom
			rdf:resource="&fibo-der-drc-bsc;OTCTransactionParty"/>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank6">
		<owl:onProperty
			rdf:resource="&fibo-fnd-pty-rl;isPlayedBy"/>
		<owl:someValuesFrom
			rdf:nodeID="blank4"/>
	</owl:Restriction>
	
	<owl:Restriction
		rdf:nodeID="blank7">
		<owl:onProperty
			rdf:resource="&fibo-fnd-rel-rel;appliesTo"/>
		<owl:someValuesFrom
			rdf:resource="&fibo-der-drc-otci;OTCInstrument"/>
	</owl:Restriction>

</rdf:RDF>