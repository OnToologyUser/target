	<owl:Class
		rdf:about="&fibo-fnd-law-lcap;ContractualCapability">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-law-lcap;LegalCapacity"/>
		<rdfs:label
			rdf:datatype="&xsd;string">contractual capability</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">The capacity to enter into legally binding contracts.</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xsd;string">This is the capacity which defines Contractually Capable Entity (sometimes labeled as &apos;Legal Entity&apos;) as distinct from &apos;Legal Person&apos;. In the latter case the liabilities incurred in the contract accrue also to the Legal Person. In the case of contractual capability, the entity has the authority to enter into contracts, whether or not the liabilities accrue to that same entity (which they do if it is also a Legal Person). For Legal Entities which are not Legal Persons, the liability unwinds to some legal person within the structure of the entity, for example a General Partner or a Trustee.</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-law-lcap;DelegatedLegalAuthority">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-law-lcap;LegalCapacity"/>
		<rdfs:label
			rdf:datatype="&xsd;string">delegated legal authority</rdfs:label>
		<fibo-fnd-utl-av:explanatoryNote
			rdf:datatype="&xsd;string">This is always accompanied by an equal responsibility for one&apos;s actions or a failure to act.</fibo-fnd-utl-av:explanatoryNote>
		<skos:definition
			rdf:datatype="&xsd;string">institutionalized and legal power inherent in a particular job, function, or position that is meant to enable its holder to successfully carry out his or her responsibilities, where such power has been delegated through some formal means</skos:definition>
		<skos:scopeNote
			rdf:datatype="&xsd;string">This specifically means the authority to make legally binding commitments.</skos:scopeNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-law-lcap;Duty">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-law-lcap;LegalConstruct"/>
		<rdfs:label
			rdf:datatype="&xsd;string">duty</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">Some obligation which exists and is imposed on some individual.</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xsd;string">This can also be thought of as an obligation - not in the sense in which an obligation and a right are the converse aspects of one another, but in and of itself, independent of the perspective from which it is considered. Examples include statutory obligations, reporting obligations and so on.</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-law-lcap;LegalCapacity">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-law-lcap;LegalConstruct"/>
		<rdfs:label
			rdf:datatype="&xsd;string">legal capacity</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">The capacity to carry out certain actions or to have certain rights.</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xsd;string">suggested definition only</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-law-lcap;LegalConstruct">
		<rdfs:subClassOf
			rdf:nodeID="blank5"/>
		<rdfs:subClassOf
			rdf:nodeID="blank6"/>
		<rdfs:label
			rdf:datatype="&xsd;string">legal construct</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">Something which is conferred by way of law or contract, such as a right.</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xsd;string">Obligations are an aspect of this category of thing, as are rights.</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-law-lcap;LiabilityCapacity">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-law-lcap;LegalCapacity"/>
		<rdfs:label
			rdf:datatype="&xsd;string">Liability Capacity</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">The ability to be sued at law</skos:definition>
		<skos:editorialNote
			rdf:datatype="&xsd;string">Note that for the purposes of this model, this is distinct from culpability (the ability to commit criminal acts). That would be a separate and analogous term but with grounding in criminal rather than civil law.</skos:editorialNote>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-law-lcap;SignatoryCapacity">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-law-lcap;LegalCapacity"/>
		<rdfs:label
			rdf:datatype="&xsd;string">Signatory Capacity</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">the capacity of some natural person to sign agreements on the part of some organization or legal person</skos:definition>
	</owl:Class>
	
	<owl:Class
		rdf:about="&fibo-fnd-law-lcap;StatutoryResponsibility">
		<rdfs:subClassOf
			rdf:resource="&fibo-fnd-law-lcap;Duty"/>
		<rdfs:subClassOf
			rdf:nodeID="blank7"/>
		<rdfs:label
			rdf:datatype="&xsd;string">statutory responsibility</rdfs:label>
		<skos:definition
			rdf:datatype="&xsd;string">An obligation which is defined under some body of law (statute).</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-law-lcap;hasCapacity">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-rel-rel;has"/>
		<rdfs:label
			rdf:datatype="&xsd;string">has capacity</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-law-lcap;LegalCapacity"/>
		<owl:inverseOf
			rdf:resource="&fibo-fnd-law-lcap;isCapacityOf"/>
		<skos:definition
			rdf:datatype="&xsd;string">identifies an individual or organization that has some capability to carry out certain actions, or has certain rights or obligations</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty
		rdf:about="&fibo-fnd-law-lcap;isCapacityOf">
		<rdfs:subPropertyOf
			rdf:resource="&fibo-fnd-rel-rel;isConferredOn"/>
		<rdfs:label
			rdf:datatype="&xsd;string">is capacity of</rdfs:label>
		<rdfs:domain
			rdf:resource="&fibo-fnd-law-lcap;LegalCapacity"/>
		<rdfs:range
			rdf:resource="&fibo-fnd-aap-agt;AutonomousAgent"/>
		<skos:definition
			rdf:datatype="&xsd;string">identifies an individual or organization on which a given legal capacity has been conferred</skos:definition>
		<skos:scopeNote
			rdf:datatype="&xsd;string">This includes capacities specific to duties at law (such as those for corporate officers) as well as the ability or capacity to incur liability.</skos:scopeNote>
	</owl:ObjectProperty>
	
